{
	"extends": [
		"eslint:recommended",
		"plugin:prettier/recommended",
		"next/core-web-vitals",
		"prettier"
	],
	"env": {
		"browser": true,
		"es2023": true
	},
	"rules": {
		"no-unused-vars": "error",
		"no-var": "error",
		"prefer-const": "warn",
		"no-console": "error",
		"no-undefined": "error",
		"no-undef": "error",
		"no-unused-expressions": "error",
		"no-unreachable": "error",
		"prettier/prettier": [
			"warn",
			{
				"endOfLine": "auto"
			}
		]
	}
}
