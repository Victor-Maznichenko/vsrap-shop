{
	"quoteProps": "as-needed",
	"bracketSameLine": false,
	"jsxSingleQuote": false,
	"bracketSpacing": true,
	"trailingComma": "all",
	"arrowParens": "avoid",
	"singleQuote": false,
	"endOfLine": "lf",
	"printWidth": 100,
	"useTabs": true,
	"semi": true,
	"tabWidth": 3,
	"plugins": ["@trivago/prettier-plugin-sort-imports"]
}
